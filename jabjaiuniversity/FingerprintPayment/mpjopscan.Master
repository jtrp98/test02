                    //    hideScan('', '8', '');
                    //}
                    //});
                }
            }).done(function (obj) {
                if (obj.length == 0) {
                    $("input[id*=txtsID]").val("");
                    $('input[id*=txtsID]').prop("disabled", false);
                    j_alert("แจ้งผลการดำเนินการ", "<lable>ไม่มีข้อมูลในระบบ</lable>");
                    return;
                }
                else {
                    $.each(obj, function (index) {
                        _check = true;
                        var ID = "";
                        var sName = obj[index].sName;
                        var sLastname = obj[index].sLastname;
                        var sFinger = obj[index].sFinger;
                        var sFinger2 = obj[index].sFinger2;

                        //if (sName != "") {
                        //    errcode = fnVerifyEx(sFinger);
                        //    if (errcode != 0) {
                        //        errcode = fnVerifyEx(sFinger2);
                        //    }
                        //}
                        if (fnVerifyEx(sFinger) == 0 || fnVerifyEx(sFinger2) == 0) {
                            if (Mode == "EmpScan") {
                                _ID = obj[index].sEmp;
                                document.getElementById("aspnetForm").ctl00$MainContent$txtCheckFinger.value = _ID;
                            }
                            else if (Mode == "JobScan") {
                                _ID = obj[index].sID;
                                document.getElementById("aspnetForm").ctl00$MainContent$txtCheckFinger.value = _ID;
                                //                                setJobScanForm(rs_addmoney.fields(0), rs_addmoney.fields(1), rs_addmoney.fields(2));
                            }
                            document.getElementById("aspnetForm").ctl00$MainContent$txtsName.value = sName;
                            document.getElementById("aspnetForm").ctl00$MainContent$txtsLastName.value = sLastname;
                            setJobScanForm(_ID, sName, sLastname);
                            return;
                        }
