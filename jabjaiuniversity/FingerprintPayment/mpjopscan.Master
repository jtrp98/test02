        function Getdata(sID, Mode) {
            $.ajax({
                type: "POST",
                url: "/App_Logic/dataGeneric.ashx?ID=" + sID + "&mode=" + Mode,
                cache: false,
                contentType: 'application/json;',
                success: function (obj) {


